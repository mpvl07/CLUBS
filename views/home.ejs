  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 400px;
  background: white;
  border-radius: 10px;
  box-shadow: 10px 10px 15px rgba(0,0,0,0.05);
}
.center h1{
  text-align: center;
  padding: 20px 20px;
  border-bottom: 1px solid silver;
}
.btn {
        
          display: block;
           width: 50%;
            padding: 10px;
            border: none;
            border-radius: 5px;
            font-size: 0.8em;
            color: #fff;
            background-color: #007bff;
            cursor: pointer;
            transition: background-color 0.2s;
          margin: 0 auto;
          text-align: center;
          align-items: center;
          justify-content: center;
          margin-top: 20px;
          margin-bottom: 20px;
}

